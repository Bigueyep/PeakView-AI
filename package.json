{
  "name": "peakview-geo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:db": "docker-compose -f docker-compose.dev.yml up db -d",
    "dev:full": "docker-compose -f docker-compose.dev.yml up -d && npm run dev",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "dev:reset": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up -d",
    "build": "next build",
    "start": "next start -p 3000",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "webhook": "node scripts/stripe-webhook.js",
    "setup": "npm install && npm run prisma:generate && npm run dev:db"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.5",
    "@prisma/client": "^5.20.0",
    "autoprefixer": "^10.4.19",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.5",
    "postcss": "^8.4.38",
    "rate-limiter-flexible": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^14.25.0",
    "tailwindcss": "^3.4.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "24.6.0",
    "@types/react": "19.1.15",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prisma": "^5.20.0",
    "ts-jest": "^29.1.1",
    "typescript": "5.9.2"
  }
}
